	"tokenColors": [
		{
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#A5A18E"
			}
		},
		{
			"scope": [
				"keyword.other",
				"storage.type",
				"meta.interpolation",
				"keyword.control",
				"support.type.property-name.table",
				"support.type.property-name.array",
				"constant.numeric",
				"entity.name.tag",
				"entity.name.namespace"
			],
			"settings": {
				"foreground": "#379ccd"
			}
		},
		{
			"scope": [
				"punctuation.comma",
				"punctuation.brackets"
			],
			"settings": {
				"foreground": "#4E4B42"
			}
		},
		{
			"scope": [
				"entity.name.type",
				"entity.other"
			],
			"settings": {
				"foreground": "#0f860b"
			}
		},
		{
			"scope": [
				"constant.language",
				"entity.name.function",
				"storage.modifier.mut",
				"support.function.builtin"
			],
			"settings": {
				"foreground": "#c77bb0"
			}
		},
		{
			"scope": [
				"entity.name.function.macro"
			],
			"settings": {
				"foreground": "#d7ac01"
			}
		},
		{
			"scope": [
				"string.quoted"
			],
			"settings": {
				"foreground": "#d7ac01"
			}
		},
		{
			"scope": [
				"constant.character.escape"
			],
			"settings": {
				"foreground": "#a38920"
			}
		}
	]
